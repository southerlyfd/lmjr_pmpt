package com.pmpt.entities.enums;

/**
 * @ClassName: Valid
 * @Description: 是否认证
 * @author: 汪洋
 * @date: 2017年8月14日 下午6:56:05
 */
public enum Valid {

	//用户实名认证
	NOTVALID("0", "未认证"),
	ISVALID("1", "已认证"),
	
	//账户或订单是否有效
	NOVALID("00","无效"),
	VALID("01","有效");
	private String key;

	private String des;

	Valid(String key, String des) {
		this.key = key;
		this.des = des;
	}

	public String getKey() {
		return key;
	}

	public void setKey(String key) {
		this.key = key;
	}

	public String getDes() {
		return des;
	}

	public void setDes(String des) {
		this.des = des;
	}

}
