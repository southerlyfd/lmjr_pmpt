package com.pmpt.entities.enums;

/**
 * @ClassName: Classification.java
 * @Description: 商品分类
 * @author jianghb
 * @date 2017年9月19日下午2:00:13
 */
public enum Classification {
	HM("01", "红木", "image/interest/xyzt.jpeg"),
	BC("02", "板材", "image/interest/fzhhl.jpg"),
	JJ("03", "家具", "image/interest/hlm.jpg");
	
	private String code;
	private String categoryName;
	private String classificationUrl;
	
	Classification(String code, String categoryName, String classificationUrl){
		this.code = code;
		this.categoryName = categoryName;
		this.classificationUrl = classificationUrl;
	}

	public String getCode() {
		return code;
	}

	public void setCode(String code) {
		this.code = code;
	}

	public String getCategoryName() {
		return categoryName;
	}

	public void setCategoryName(String categoryName) {
		this.categoryName = categoryName;
	}

	public String getClassificationUrl() {
		return classificationUrl;
	}

	public void setClassificationUrl(String classificationUrl) {
		this.classificationUrl = classificationUrl;
	}
	
}
